{
  "name": "cesium",
  "version": "1.0.6",
  "description": "Unhosted webapp client for Duniter network",
  "repository": {
    "type": "git",
    "url": "git@git.duniter.org:clients/cesium/cesium.git"
  },
  "scripts": {
    "postinstall": "bower install --allow-root",
    "install-platforms": "ionic state restore",
    "start": "ionic serve",
    "docker:build": "sudo docker build . -t cesium/release",
    "docker:run": "sudo docker run -ti --rm -p 8100:8100 -p 35729:35729 -v .:/cesium:rw cesium/release",
    "build:web": "config --env default && gulp build:web --release"
  },
  "keywords": [
    "duniter",
    "cesium",
    "ionic",
    "angular",
    "cordova",
    "crypto-currency"
  ],
  "author": "Duniter team",
  "license": "GPL-3.0",
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://git.duniter.org/clients/cesium/cesium/issues",
    "new": "https://git.duniter.org/clients/cesium/cesium/issues/new"
  },
  "dependencies": {
