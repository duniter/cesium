{
  "manifest_version": 3,
  "name": "Cesium",
  "version": "1.7.12",
  "default_locale": "fr",
  "description": "Cesium Wallet for Äž1 libre currency",
  "icons": {
    "32": "img/logo_32px.png",
    "96": "img/logo_96px.png",
    "144": "img/logo_144px.png",
    "192": "img/logo.svg"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "file:///*"
  ],
  "theme": {
    "colors": {
      "frame": "#1a237e",
      "toolbar": "#1a237e",
      "ntp_background": "#1a237e",
      "ntp_text": "#1a237e",
      "button_background": "#1a237e"
    }
  },
  "protocol_handlers": [
    {
      "protocol": "june",
      "uriTemplate": "/#/app/home?uri=%s"
    },
    {
      "protocol": "web+june",
      "uriTemplate": "/#/app/home?uri=%s"
    }
  ]
}
