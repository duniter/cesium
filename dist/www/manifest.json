{
  "manifest_version": 3,
  "name": "Cesium",
  "version": "1.7.15",
  "description": "Cesium Wallet for Äž1 libre currency",
  "icons": {
    "32": "img/logo_32px.png",
    "60": "img/logo_60px.png",
    "96": "img/logo_96px.png",
    "144": "img/logo_144px.png",
    "192": "img/logo.svg"
